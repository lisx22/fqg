<script type="text/template" id="tpl_sku_list">
    <li>
        <a class="js-sku"
           href="<% if(group['fe_super_link']) { %><%=group['fe_super_link']%><% } else {%>//item.fenqile.com/<%= group['sku'] %>.html<% } %>"
           data-fql-stat="HUI.MAIN.INDEX.SKU_<%= group['sku'] %>" target="_blank">
            <div class="list-img fn-left">
                <img width="180" alt="" title="" class="js-lazyload" data-original="<%= group['fe_sku_pic']%>"
                     src="//cres.fenqile.com/res/img/global/default_bg.png">
                <% if(group['submark_desc']) { %>
                <em class="img-icon  <% if(group['tag'] == '必抢'){ %>img-rob<% } %>"><% if(group['submark_desc'] ==
                    '必抢'){ %><i></i><% }%><%= group['submark_desc']%></em>
                <% } %>
                <em class="em-move"></em>
            </div>
            <div class="list-con">
                <div class="title">
                    <h4><%= group['product_name']%></h4>
                    <p class="sub-head"><%= group['product_desc']%></p>
                </div>
                <div class="payment">
                    <span class="yg">月供：<em class="fn-rmb">¥<%= group['mon_pay']%></em>起</span>
                    <% if(group['active_status'] == 2 && group['sku_num'] != 0) { %><!--状态（1：未开始2：进行中3：已结束）-->
                    <span class="gz">仅剩<em><%= group['sku_num'] %></em>件</span>
                    <% } else if (group['active_status'] == 2 && group['sku_num'] == 0) { %>
                    <span class="gz">库存紧张</span>
                    <% } else if (group['active_status'] == 1) { %>
                    <span class="gz">仅限<em><%= group['sku_num'] %></em>件</span>
                    <% } %>
                </div>
                <div class="purchase">
                    <p><span class="fn-rmb">¥</span><em><%= group['real_amount']%></em>
                        <del>¥<%= group['amount']%></del>
                    </p>
                    <% if(group['fe_super_status']) { %><!--先判断是否超级单品逻辑-->
                    <span class="pur grab"><%= group['fe_super_btn_text'] %></span>
                    <% }else{ %>
                    <% if(group['active_status'] == 1) { %><!--状态（1：未开始2：进行中3：已结束）-->
                    <span class="pur remind js-remind-btn" data-goods-name="<%= group['goods_name'] %>"
                          data-begin-time="<%= group['begin_time'] ? group['begin_time'] : 0; %>"
                          data-amount="<%= group['real_amount'] %>" data-plate-id="<%= group['plate_id'] %>"
                          data-mart-amount="<%= group['mart_amount'] %>" data-goods-id="<%= group['id'] %>"
                          data-sku="<%= group['sku'] %>">提醒我</span>
                    <% } else if (group['active_status'] == 2) { %>
                    <span class="pur grab">去抢购</span>
                    <% } else if (group['active_status'] == 3) { %>
                    <span class="pur over">已抢完</span>
                    <% } %>
                    <% } %>
                </div>
            </div>
        </a>
    </li>
</script>

