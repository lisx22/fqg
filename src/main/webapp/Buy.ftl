<script type="text/template" id="tpl_coupon_popup">
    <% if(coupon_list.length > 0){ %>
    <div>
        <div class="fn-text-center coupon-overflow">
            <ul class="new-coupon-list coupon-fql fn-clear coupon-layer">
                <% for(var i in coupon_list){
                        var cls = coupon_list[i].receive_status == 2 ? 'li-quan-disabled' : '';
                        var txt = coupon_list[i].receive_status == 2 ? '已领取' : '立即领取';
                        var is_lehei_coupon = coupon_list[i].fe_is_lehei;
                %>
                    <li class="js-coupon-li js-coupon-<%= coupon_list[i].tps_key; %> <%= cls; %> <%= is_lehei_coupon ? 'lehei' : ''%>" data-key="<%= coupon_list[i].tps_key; %>">
                        <div class="coupon-left">
                            <p class="coupon-top">
                                <strong class="fn-rmb price">&yen;<%= coupon_list[i].amount; %></strong>
                                <span class="coupon-state"><%= coupon_list[i].cost_type == 2 ? "店铺券" : "现金券" %></span>
                            </p>
                            <p class="coupon-text">单笔消费满<%= coupon_list[i].amount_limit; %>元可用</p>
                            <p class="coupon-time">
                                <%
                                    if(coupon_list[i].use_time_type == 2){ //use_time_type: 1 绝对时间,2 相对时间；配置优惠券时有效时间是相对时间展示有效期是xx天内，有效期是绝对时间展示具体日期
                                %>
                                    领取后<%= coupon_list[i].use_left_time_day;%>天内有效
                                <%  } else {%>
                                    <%= coupon_list[i].start_valid; %>-<%= coupon_list[i].end_valid; %>
                                <%  } %>
                                <% if ( is_lehei_coupon ) { %>
                                <span class="lehei-txt"><%= coupon_list[i].user_type_get_desc; %></span>
                                <%  } %>
                            </p>
                        </div>
                        <div class="coupon-right">
                            <p class="coupon-info"><%= coupon_list[i].limit_desc; %></p>
                            <span class="btn-coupon js-disabled-<%= coupon_list[i].tps_key; %>"><%= txt; %></span>
                        </div>
                    </li>
                <% } %>
            </ul>
        </div>
    </div>
    <% } %>
</script>

