<script>
    $(function () {
//        获取分期
        $(".cl_1").click(function () {
            alert($(this).parent().val());
            $(this).parent().addClass("id_1").siblings().removeClass("id_1");
            add();
        });
        $(function () {
            $("#btn_buy").click(function () {
//               点击购买
                var idd = ${commodityInfo.commodityId};
                var str = $(".id_1").val();
                var color = $("#color").html();
                var pay = $("#mon_pay").html();
                var result = "1";
                var pass = prompt("请输入支付密码","");
                var buyInfo = idd+"-"+str+"-"+color+"-"+pay+"-"+result+"-"+pass;
                $.post(
                        "buy/addorder",
                        {"buyInfo":buyInfo},
                        function (data) {
                            if(data=="1"){
                                alert("您还未登录");
                                window.location.href = "Buy.ftl";
                            }else{
                            }
                        }
                )
                add();
            });
        });
        function add() {
          $("#mon_pay").each(function () {
              var lengt = $("#amount").text();
              var le =  parseFloat(lengt)*1000;
              var str =$(".id_1").val();
              var strr =  parseFloat(str);
              var len =parseInt(le/strr);
              $(this).html(len);
          });
        };
        add();
    });
</script>