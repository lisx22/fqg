<script type="text/template" id="tpl_gift_list">
    <div class="promotion detail-pay-month">
        <div class="tdlet2"><span class="price label-pol"><span class="letter2">促销</span><span class="letter-pl">：</span></span></div>
        <div>
            <ul>
                <% var is_show_more = false; %>
                <!--若仅有一个赠品 则直接展示-->
                <% if(gift_list.length == 1 && gift_list[0].type == 0) {%>
                <li>
                    <span class="bor-red"><%= gift_list[0]['name']; %></span>
                    <!--若促销类型为赠品 且 赠品存在sku_id则需要添加跳转链接-->
                    <% if(gift_list[0]['type'] == 0 && gift_list[0]['sku_id']) {%>
                        <a class="bor-link" target="_blank" href="<%= '//item.fenqile.com/' + gift_list[0]['sku_id'] + '.html'; %>"><%= gift_list[0]['desc']; %> <%= gift_list[0]['limit_num'] ? 'x' + gift_list[0]['limit_num'] : ''; %></a>
                    <% }else { %>
                        <%= gift_list[0]['desc']; %> <%= gift_list[0]['limit_num'] ? 'x' + gift_list[0]['limit_num'] : ''; %>
                    <% }%>
                </li>
                <% }else {
                    if(gift_icon_list[0]['type'] == 0 && gift_icon_list[0]['count'] > 1){
                        is_show_more = true;
                    }
                %>
                <li class="js-all-promotion-list shut_li">
                    <span class="bor-red"><%= gift_icon_list[0]['name']; %></span>
                    <span><%= gift_icon_list[0]['desc']; %></span>
                </li>
                <!--可折叠的图标列表 当促销类型为两种 且 赠品类型只有一个时需要直接展示 -->
                <% if(gift_icon_list.length > 1) {%>
                <li class="js-all-promotion-list shut_li">
                    <% gift_icon_list = gift_icon_list.slice(1);%>
                    <% if(gift_icon_list.length == 1) {%>
                        <span class="bor-red"><%= gift_icon_list[0]['name']; %></span>
                        <!--若促销类型为赠品 且 赠品存在sku_id则需要添加跳转链接 且赠品为1个-->
                        <% if(gift_icon_list[0]['type'] == 0 && gift_icon_list[0]['sku_id'] && gift_icon_list[0]['count'] == 1) {%>
                            <a class="bor-link" target="_blank" href="<%= '//item.fenqile.com/' + gift_icon_list[0]['sku_id'] + '.html'; %>"><%= gift_icon_list[0]['desc']; %> <%= gift_icon_list[0]['limit_num'] ? 'x' + gift_icon_list[0]['limit_num'] : ''; %></a>
                        <% }else { %>
                            <%= gift_icon_list[0]['desc']; %>
                        <% }%>
                    <% }else {
                    for(var i in gift_icon_list) {%>
                        <span class="bor-red"><%= gift_icon_list[i]['name']; %></span>
                    <% }}%>
                </li>
                <% }%>
                <!--隐藏输出全部促销列表-->
                <% for(var i in gift_list) {%>
                    <li class="fn-hide js-all-promotion-list">
                        <span class="bor-red"><%= gift_list[i]['name']; %></span>
                        <% if(gift_list[i]['type'] == 0 && gift_list[i]['sku_id']) {%>
                            <a class="bor-link" target="_blank" href="<%= '//item.fenqile.com/' + gift_list[i]['sku_id'] + '.html'; %>"><%= gift_list[i]['desc']; %> <%= gift_list[i]['limit_num'] ? 'x' + gift_list[i]['limit_num'] : ''; %></a>
                        <% }else { %>
                            <%= gift_list[i]['desc']; %> <%= gift_list[i]['limit_num'] ? 'x' + gift_list[i]['limit_num'] : ''; %>
                        <% }%>
                    </li>
                <% }%>

                <% }%>
            </ul>
            <!--若促销数目大于2 或 被判定为需展促销详情-->
            <% if(gift_list.length > 2 || is_show_more) {%>
                <i class="pro-st" id="promotion_arrow"></i>
            <% }%>
        </div>
    </div>
</script>
